{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "@odata.id": "HpeInvalidImage",
    "additionalProperties": false,
    "copyright": "Copyright 2014-2019 Hewlett Packard Enterprise Development, LP.  Portions Copyright 2014-2019 Distributed Management Task Force. All rights reserved.",
    "description": "The HpeInvalidImage resource describes the properties of an invalid firmware image detected and preserved from a computer system.",
    "properties": {
        "@odata.context": {
            "$ref": "Resource.json#/definitions/odatacontext"
        },
        "@odata.etag": {
            "$ref": "Resource.json#/definitions/odataetag"
        },
        "@odata.id": {
            "$ref": "Resource.json#/definitions/odataid"
        },
        "@odata.type": {
            "$ref": "Resource.json#/definitions/odatatype"
        },
        "Created": {
            "description": "ISO 8601 time string indicating when this image was added to the NAND.",
            "format": "date-time",
            "readonly": true,
            "type": "string"
        },
        "DeviceClass": {
            "description": "Device type GUID from iLO secure flash header",
            "pattern": "([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})",
            "readonly": true,
            "type": "string"
        },
        "Filename": {
            "description": "The unique filename of the image.",
            "maxLength": 64,
            "pattern": "^[A-Za-z0-9-._]+$",
            "readonly": true,
            "type": "string"
        },
        "Filepath": {
            "description": "Path of file on the USB LUN if the repo is mounted as USB.",
            "readonly": true,
            "type": "string"
        },
        "Id": {
            "$ref": "Resource.json#/definitions/Id"
        },
        "ImageUri": {
            "description": "URI of the image binary.",
            "format": "uri-reference",
            "readonly": true,
            "type": "string"
        },
        "Name": {
            "description": "Name of the image.",
            "maxLength": 64,
            "pattern": "^[A-Za-z0-9-._\\(\\)\\, ]+$",
            "readonly": true,
            "type": "string"
        },
        "SizeBytes": {
            "description": "Size of the image file in bytes.",
            "minimum": 0,
            "readonly": true,
            "type": [
                "integer",
                "null"
            ]
        }
    },
    "title": "#HpeInvalidImage.v1_0_0.HpeInvalidImage",
    "type": "object"
}